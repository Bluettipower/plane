{
  "extends": "tsconfig/nextjs.json",
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "next-i18next.config.js"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "*"
      ]
    }
  }
}